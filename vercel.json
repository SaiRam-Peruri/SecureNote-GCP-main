{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ATLAS_URL": "ATLAS_URL",
    "SESSION_SECRET": "SESSION_SECRET",
    "APP_NAME": "APP_NAME",
    "HOST_URL": "HOST_URL",
    "VERCEL_URL": "VERCEL_URL"
  }
}
